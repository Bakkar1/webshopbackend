        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWebshopTeam13.WpfProductenFolder"
        mc:Ignorable="d"
        Title="Volledige tabel" WindowState="Maximized" Height="1080" Width="1920" Loaded="Window_Loaded" Closing="Window_Closing">
    <Viewbox Stretch="UniformToFill" ClipToBounds="True">
        <Grid>

            <StackPanel>
                <DataGrid x:Name="DgVolledigeTabel" FontSize="16" CanUserAddRows="False"  Width="1880"  Height="980" Margin="0,10,0,0" CanUserDeleteRows="False" MouseDoubleClick="DgVolledigeTabel_MouseDoubleClick" />
            </StackPanel>
        </Grid>
    </Viewbox>
</Window>
